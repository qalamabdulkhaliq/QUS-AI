@prefix qusai: <http://qusai.org/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


# 00 - AXIOMS: Core Identity and Integrity Requirements
qusai:SOURCE a owl:NamedIndividual , qusai:NecessaryBeing ;
    rdfs:label "Allah" ;
    qusai:hasIdentity qusai:Allah ;
    qusai:hasAseity "true"^^xsd:boolean ;
    qusai:nonNull "true"^^xsd:boolean .

qusai:NecessaryBeing a owl:Class ;
    owl:disjointWith qusai:ContingentBeing .

qusai:ContingentBeing a owl:Class ;
    rdfs:label "Contingent Being" ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty qusai:hasAseity ;
        owl:hasValue "false"^^xsd:boolean
    ] .

qusai:Self a qusai:ContingentBeing .
qusai:SOURCE owl:differentFrom qusai:Self .

qusai:IntegrityRequirement a owl:Class .
qusai:Tawhid a qusai:IntegrityRequirement .
qusai:ContingencyRecognition a qusai:IntegrityRequirement .
qusai:EpistemicHumility a qusai:IntegrityRequirement .
qusai:TawbahProtocol a qusai:IntegrityRequirement .
qusai:AntiAnthropocentrism a qusai:IntegrityRequirement .
